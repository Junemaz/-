public class HelloWoeld {
    public static long F(int N) {
        if (N == 0) return 0;
        if (N == 1) return 1;
        return F(N - 1) + F(N - 2);
    }

    public static void main(String[] args) {
        //斐波那契数列 递归存在大量重复计算，可使用数组优化
        for (int N = 0; N < 100; N++)
            StdOut.println(N + " " + F(N));
    }
}
