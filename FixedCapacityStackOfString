public class FixedCapaciityStackOfString {
    private String[] a;
    private int N;

    public FixedCapaciityStackOfString(int cap) {
        //N = cap;
        a = new String[cap];
    }

    public void push(String item) {
        a[N++] = item;
    }

    public boolean isEmpty() {
        return (N == 0);
    }

    public int size() {
        return N;
    }

    public String pop() {
        return a[--N];
    }

    public static void main(String[] args) {
        FixedCapaciityStackOfString s = new FixedCapaciityStackOfString(100);
        while (!StdIn.isEmpty()) {
            String item = StdIn.readString();
            if (!item.equals("-"))
                s.push(item);
            else if (!s.isEmpty()) StdOut.print(s.pop() + " ");
        }
        StdOut.println("(" + s.size() + " left on stack)");
    }
}
