public class HelloWoeld {
    public static void main(String[] args) {
        System.out.println("Hello World! \n");

        int[] a = new int[10];
        for (int i = 0; i < 10; i++)
            a[i] = 9 - i;
        for (int i = 0; i < 10; i++)
            a[i] = a[a[i]];
        for (int i = 0; i < 10; i++)
            System.out.println(i);         //printf i 也太搞了吧
            //输出0-9  
    }
}
