public class HelloWoeld {

    public static int log(int a) {
        int k = 0, eum = 1;

        if (a < 0)
            return -1;
        else if (a < 1)
            return 0;
        else {
            while (eum <= a) {
                eum *= 2;
                k++;
            }
            return k - 1;
        }
    }

    public static void main(String[] args) {
        System.out.println("Hello World! \n");
        int i = log(StdIn.readInt());
        StdOut.println(i);

    }
}
