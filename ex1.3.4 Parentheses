public class Stack<Item> //implements Iterable<Item>
{
    private Node first;
    private int N;

    private class Node {
        Item item;
        Node next;
    }

    public boolean isEmpty() {
        return first == null;
    }

    public int size() {
        return N;
    }


    public void push(Item item) {
        Node oldfirst = first;
        first = new Node();
        first.next = oldfirst;
        first.item = item;
        N++;
    }

    public Item pop() {
        Item item = first.item;
        first = first.next;
        N--;
        return item;
    }
/*
    public Iterator<Item> iterator() {
        return new ReverseArrayIterator();
    }

    private class ReverseArrayIterator implements Iterator<Item> {
        //支持后进先出的迭代
        private int i = N;

        public boolean hasNext() {
            return i > 0;
        }

        public Item next() {
            return a[--i];
        }

        public void remove() {

    }
    */


    public static void main(String[] args) {
        Stack<String> s = new Stack<String>();
        boolean judge = true;
        String readitem = StdIn.readString();
        String[] items = readitem.split("");
        for (String item : items) {

            if (item.equals("{") || item.equals("[") || item.equals("("))
                s.push(item);
            else if (item.equals("}") || item.equals("]") || item.equals(")")) {
                String temp = s.pop();
                if (!((item.equals("}") && temp.equals("{")) || (item.equals("]") && temp
                        .equals("[")) || (item.equals(")") && temp.equals("("))))
                    judge = false;

            }
        }
        StdOut.println(judge);
    }
}
